{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        [
          "prismjs",
          {
            "languages": ["javascript", "css", "html"],
            "plugins": ["line-numbers", "show-language", "keep-markup"],
            "theme": "okaidia",
            "css": true
          }
        ],
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          }
        ]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            }
          }
        ]
      ],
      "plugins": [
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true
          }
        ]
      ]
    }
  }
}
